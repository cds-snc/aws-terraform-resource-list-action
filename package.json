  "devDependencies": {
    "@vercel/ncc": "0.36.1",
    "ava": "4.3.3",
    "c8": "7.13.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-security": "1.7.1",
    "husky": "8.0.3",
    "prettier": "2.8.4"
  }
}